<h1>Listing enquiries</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Level</th>
      <th>Location</th>
      <th>Duration</th>
      <th>Start date</th>
      <th>Tuition fee</th>
      <th>Note</th>
      <th>Availability</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enquiries.each do |enquiry| %>
      <tr>
        <td><%= enquiry.subject %></td>
        <td><%= enquiry.level %></td>
        <td><%= enquiry.location %></td>
        <td><%= enquiry.duration %></td>
        <td><%= enquiry.start_date %></td>
        <td><%= enquiry.tuition_fee %></td>
        <td><%= enquiry.note %></td>
        <td><%= enquiry.availability %></td>
        <td><%= link_to 'Show', enquiry %></td>
        <td><%= link_to 'Edit', edit_enquiry_path(enquiry) %></td>
        <td><%= link_to 'Destroy', enquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enquiry', new_enquiry_path %>
